{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/spa"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/.well-known/apple-developer-merchantid-domain-association",
      "destination": "/api/apple-pay-domain-association"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
